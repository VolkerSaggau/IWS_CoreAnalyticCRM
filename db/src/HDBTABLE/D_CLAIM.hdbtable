column table "D_CLAIM" (
"CLAIM_ID"    INTEGER   NOT NULL   comment 'Claim ID',
"CLAIM_NBR"    VARCHAR(18)    comment 'Claim ID From External Operational Database',
"CLAIM_DESC"    VARCHAR(255)    comment 'Claim Description',
"CLAIM_TYPE"    CHAR(3)    comment 'Claim Type',
"CLM_AMT_BND"    VARCHAR(50),
"REINS_AMT_BND"    VARCHAR(50),
"INSC_ALL_ENTR_IND"    CHAR(1)    comment 'All Entries Indicator',
primary key cpbtree (CLAIM_ID)
)
 comment 'PC Claim'